{
  "name": "fantastic-doodle",
  "version": "1.0.0",
  "description": "Ramda js snippets",
  "main": "index.js",
  "scripts": {
    "spec": "cross-env mocha --require @babel/register --reporter spec",
    "test": "npm run spec",
    "format": "prettier-standard --format",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "docs": "jsdoc2md ./index.js",
    "lint-staged": {
      "*": [
        "prettier-standard --lint"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCotton/fantastic-doodle.git"
  },
  "keywords": [
    "ramdajs"
  ],
  "author": "Andrew Walpole",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TCotton/fantastic-doodle/issues"
  },
  "type": "module",
  "homepage": "https://github.com/TCotton/fantastic-doodle#readme",
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "@babel/register": "7.13.16",
    "@types/chai": "4.2.17",
    "@types/jsdoc-to-markdown": "7.0.0",
    "@types/node": "15.0.1",
    "@types/ramda": "0.27.40",
    "babel-plugin-inline-json-import": "0.3.2",
    "chai": "4.3.4",
    "cross-env": "7.0.3",
    "husky": "6.0.0",
    "jsdoc": "3.6.6",
    "jsdoc-to-markdown": "7.0.1",
    "lint-staged": "10.5.4",
    "mocha": "8.3.2",
    "modern-node": "2.9.0",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "prettier-standard": "16.4.1",
    "standard": "16.0.3",
    "testem": "3.4.1"
  },
  "dependencies": {
    "ramda": "0.27.1"
  },
  "lint-staged": {
    "*.--format": "prettier --write"
  }
}
